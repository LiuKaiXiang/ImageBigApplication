<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/common_multistateviewLin"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.yykaoo.photolibrary.photo.widget.actionbar.AsPhoToolbar
        android:id="@+id/common_multiStateView_photoolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <com.yykaoo.photolibrary.photo.widget.MultiStateView
        android:id="@+id/common_multiStateView_phoview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_content"
        app:msv_emptyView="@layout/comm_pho_emptyview"
        app:msv_errorView="@layout/comm_pho_errorview"
        app:msv_loadingView="@layout/comm_pho_loadingview"
        app:msv_viewState="content"/>

</LinearLayout>